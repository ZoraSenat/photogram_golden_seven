<h1>Edit photo</h1>

<form action="http://localhost:3000/photos/<%= @photo.id %>/update">
  <p><label for="photo_caption">Caption:</label>
  <input id="photo_caption" type="text" name="the_caption" value="<%= @photo_caption %>"</p>

    <p><label for="photo_source">Image URL:</label>
  <input id="photo_source" type="text" name="the_source" value="<%= @photo_source %>"></p>

  <button>Update Photo</button>
</form>
